{
  "name": "webpack4-learn",
  "version": "0.0.1",
  "description": "learn webpack4 config change",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack --config build/webpack.prod.config.js --mode production --report",
    "dev": "cross-env NODE_ENV='development' webpack-dev-server --config build/webpack.dev.config.js --mode development --hot",
    "dll": "webpack --config build/webpack.dll.config.js --mode development",
    "start": "npm run dll && npm run dev",
    "serve": "http-server ./dist -o"
  },
  "keywords": [
    "webpack4",
    "cli"
  ],
  "author": "zr",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
